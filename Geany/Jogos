class Jogos {
    private int id_jogo;
    private String nome_jogo;
    private String categoria;
    private String estudio;
    private double preco;

    Jogos() {
        setId(0);
        setNome("");
        setCategoria("");
        setEstudio("");
        setPreco(0.0);
    }

    public void setId(int i) {
        this.id_jogo = i;
    }

    public void setNome(String n) {
        this.nome_jogo = n;
    }

    public void setCategoria(String n) {
        this.categoria = n;
    }

    public void setEstudio(String n) {
        this.estudio = n;
    }

    public void setPreco(double p) {
        this.preco = p;
    }

    public int setId() {
        return this.id_jogo;
    }

    public String setNome() {
        return this.nome_jogo;
    }

    public String setCategoria() {
        return this.categoria;
    }

    public String setEstudio() {
        return this.estudio;
    }

    public double setPreco() {
        return this.preco;
    }

}
